      "build_path": "tree-sitter-python",
      "parser_name": "python"
    },
    "javascript": {
      "extensions": [".js", ".jsx", ".mjs", ".cjs"],
      "repository": "https://github.com/tree-sitter/tree-sitter-javascript",
      "build_path": "tree-sitter-javascript",
      "parser_name": "javascript"
